// CallTest.jack
class CallTest {

    field int x, y;

    function void main() {
        var int a;
        var CallTest obj;
        var String s;

        let a = 10;               // simple variable
        let obj = this;            // object variable

        do Output.printInt(a);     // simple call to current object
        do Output.printInt(obj.add(a)); // method call on object variable
        do Output.printInt(CallTest.staticAdd(a, 5)); // static/class call
        let s = String.new(3);    // constructor call
        do s.appendChar(72);      // method call on object (String)
    }

    method int add(int n) {
        return n + x;             // returns n + field x
    }

    function int staticAdd(int p, int q) {
        return p + q;             // returns sum
    }
}